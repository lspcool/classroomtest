<% if group_assignment_repos.present? %>
    <% group_assignment_repos.each do |group_assignment_repo| %>
        <%= render partial: 'orgs/group_assignment_repos/group_assignment_repo', locals: { 
            url: organization_group_assignment_group_assignment_repo_path(organization, group_assignment, group_assignment_repo) 
        } %>
    <% end %>
<% else %>
    <div class="blankslate">
        <h3>"<%= group_assignment.title %>" does not have any repositories matching this query.</h3>
    </div>
<% end %>
